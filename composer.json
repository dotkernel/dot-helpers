{
  "name": "dotkernel/dot-helpers",
  "type": "library",
  "description": "Various helper structures and classes",
  "license": "MIT",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "psr/http-message": "^1.0",
    "zendframework/zend-servicemanager": "^3.3.0",
    "zendframework/zend-expressive-helpers": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3",

    "zendframework/zend-diactoros": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Dot\\Helpers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Helpers\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2-dev",
      "dev-develop": "0.3-dev"
    }
  }
}
